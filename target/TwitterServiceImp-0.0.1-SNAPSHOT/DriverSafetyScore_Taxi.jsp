<%!
   public int AvgOverspeed(int x){
       int score1;
    	if(x>20)
       score1=0;
       else if(x>15)
       score1=10;
       else if(x>10)
       score1=20;
       else if(x>5)
    	score1=35;
       else
    	   score1=40;
    	
    	return score1;
    }
    %>
    
    <%! 
  public int SuddenAccelerationTimes(int y)
     {
    	int score2;
    	if(y>6)
         score2=0;
    	else if(y>4)
        score2=4;
    	else if(y>2)
    	score2=8;
    	else
    	score2=10;
    	
    	return score2;
    		
    }
  %>
  
  <%!
  
    public int SuddenBrakeTimes(int z)
    {
    	int score3;
    	
    	if(z>6)
    	score3=0;
    	else if(z>4)
        score3=4;
    	else if(z>2)
    	score3=8;
    	else
    	score3=10;
    	
    	return score3;
    }
    %>
    
   <%!
    public int DistractedTimes(int p)
    {
		int score4;
		
    	if(p>32)
    		score4=0;
    	else if(p>22)
        	score4=10;
    	else if(p>2)
    		score4=30;
    	else
    	score4=40;
    	
    	return score4;
    }
